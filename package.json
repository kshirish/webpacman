{
  "name": "webpacman",
  "version": "1.0.0",
  "description": "hands on webpack",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --progress --colors --open --config webpack.config.dev.js",
    "watch": "node_modules/.bin/webpack --progress --colors --watch",
    "build": "NODE_ENV=production node_modules/.bin/webpack --progress --colors --config webpack.config.prod.js",
    "server": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshirish/webpacman.git"
  },
  "keywords": [
    "Webpack"
  ],
  "author": "kshirish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kshirish/webpacman/issues"
  },
  "homepage": "https://github.com/kshirish/webpacman#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.7",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
